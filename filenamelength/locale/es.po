msgid "Directory error"
msgstr "Error de directory"

msgid "ordered by path"
msgstr "ordenado por path"

msgid "ordered by path length"
msgstr "ordenado por la longitud del path"

msgid "ordered by filename length"
msgstr "ordenado por la longitud del nombre del fichero"

msgid "{} files found {}, whose path length is greater than or equal to {} and its filename length is greater than or equal to {}"
msgstr "{} ficheros encontrados {}, cuya longitud de path es mayor o igual que {} y su longitud de nombre de fichero es mayor o igual que {}"

msgid "Lists files with path and filename conditions"
msgstr "Lista ficheros con condiciones de path y nombre de fichero"

msgid "Minimum length for windows is 247"
msgstr "La mínima longitud para windows es 247"

msgid "Developed by Mariano Muñoz 2019-{}"
msgstr "Desarrollado por Mariano Muñoz 2019-{}"

msgid "List files whose path length is greater than or equal to this value"
msgstr "Lista ficheros cuya longitud de path es mayor o igual a este valor"

msgid "List files whose filename length is greater than or equal to this value"
msgstr "Lista ficheros cuya longitud de nombre de fichero es mayor o igual a este valor"

msgid "Different ways to order output"
msgstr "Diferentes formas de ordenar la salida"

msgid "New Release:"
msgstr "Nueva versión:"

msgid "  * Change version and date in __init__.py"
msgstr "  * Cambiar versión y fecha en __init__.py"

msgid "  * Change version in pyproject.toml"
msgstr "  * Cambiar versión en pyproject.toml"

msgid "  * Make a new tag in github"
msgstr "  * Crear una nueva etiqueta en github"

msgid "  * Create a new gentoo ebuild with the new version"
msgstr "  * Crear un nuevo ebuild de gentoo con la nueva versión"

msgid "  * Upload to portage repository"
msgstr "  * Subir al repositorio portage"
